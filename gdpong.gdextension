[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://gdpong-rust/target/debug/libgdpong_rust.so"
linux.release.x86_64 =   "res://gdpong-rust/target/release/libgdpong_rust.so"
windows.debug.x86_64 =   "res://gdpong-rust/target/debug/gdpong_rust.dll"
windows.release.x86_64 = "res://gdpong-rust/target/release/gdpong_rust.dll"
macos.debug =            "res://gdpong-rust/target/debug/libgdpong_rust.dylib"
macos.release =          "res://gdpong-rust/target/release/libgdpong_rust.dylib"
macos.debug.arm64 =      "res://gdpong-rust/target/debug/libgdpong_rust.dylib"
macos.release.arm64 =    "res://gdpong-rust/target/release/libgdpong_rust.dylib"